$: note("a3 e4 a3 c4!8")
  .s("white")
  .decay(sine.slow(2) 
  .range(0.05,1))
  .gain(.1)
  .dist(2)
  .sometimesBy(.1,x=>x.ply("4|8|16|32"))
  .lpf(sine.fast(2)
  .range(500,10000))
  .punchcard()
$: s("bd!4") .bank("linndrum").gain(2)
$: s ("hh oh sd hh hh oh sd*2 hh*4").bank("linndrum").gain(.5)
